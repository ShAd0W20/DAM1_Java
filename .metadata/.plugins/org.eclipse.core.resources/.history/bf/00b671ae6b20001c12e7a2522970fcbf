import java.util.Scanner;

public class SimpleSelection {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		
		int clientMoney = 0;
		float descompte = 0.10;
		int finalPrice = 0;
		
		System.out.println("Quants diners has pagat?");
		
		clientMoney = input.nextInt();
		input.nextLine();
		
		if(clientMoney >= 100) {
			finalPrice = (int) (clientMoney * descompte);
		}

	}

}

import java.util.ArrayList;

public class Array {

	public static void main(String[] args) throws InterruptedException {
		String[][] cars = { {"Audi", "BMW", "Aston Martin", "Ferrari", "Lambo"}, {"Suka", "Blyat", "Rush", "Aaaaa"} };
		ArrayList<String> numbers = new ArrayList<String>();

		for (String[] i : cars) {
			for(String[] j : cars) {
				Thread.sleep(100);
				System.out.println(j);
			}
		}

		numbers.add("One");
		numbers.add("Two");
		numbers.add("Three");

		numbers.forEach((n) -> {
			try {
				Thread.sleep(300);
				System.out.println(n);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		});

	}

}
